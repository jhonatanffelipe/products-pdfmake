{
  "name": "rentx",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/jhonatanffelipe/rentx.git",
  "author": "jhonatanffelipe <jhonatanfnm@gmail.com>",
  "license": "MIT",
  "scripts": {
    "up": "docker-compose -f docker-compose.yml up",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node-modules --respawn src/shared/infra/http/server.ts",
    "migration:create": "ts-node-dev ./node_modules/typeorm/cli.js  migration:create -n ",
    "migration:run": "ts-node-dev ./node_modules/typeorm/cli.js  migration:run",
    "migration:revert": "ts-node-dev ./node_modules/typeorm/cli.js  migration:revert",
    "seed:create": "ts-node-dev ./node_modules/typeorm/cli.js migration:create -c seed -n ",
    "seed:run": "ts-node-dev ./node_modules/typeorm/cli.js migration:run -c seed",
    "seed:revert": "ts-node-dev ./node_modules/typeorm/cli.js migration:revert -c seed",
    "test": "NODE_ENV=test jest --runInBand"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "csv-parse": "^5.0.4",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.3",
    "multer": "^1.4.4",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.2.3",
    "swagger-ui-express": "^4.4.0",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.2.37",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
}
